<%@page pageEncoding="UTF-8" %>
<%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<c:choose>
    <c:when test="${pageContext.request.getParameter('view') eq 'new'}">
        <c:if test="${rubrication ne null}"><div align="left"><%@include file="rubricator/simple_script_new.jspf"%></div></c:if>
    </c:when>
    <c:otherwise>
        <c:if test="${rubrication ne null}"><div align="left"><%@include file="rubricator/simple_script.jspf"%></div></c:if>
    </c:otherwise>
</c:choose>