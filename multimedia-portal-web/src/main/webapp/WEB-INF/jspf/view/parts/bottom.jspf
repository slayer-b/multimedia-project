<%@ page pageEncoding="UTF-8" %>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<table class="counters" width="100%" cellpadding="0" cellspacing="0"><tr><c:forEach items="${counters}" var="counter">
<td>${counter.text}</td></c:forEach><td width="100%"/><td><div class="adv_bottom">${advertisement['bottom']}</div></td></tr></table>
<div class="bottom_rights" align="justify"><fmt:message bundle="${template}" key="bottom_rights"/></div>
<div align="right" class="bottom_logo"><a href="${pageContext.servletContext.contextPath}/" title="${site_config.optimization_phrase}">${site_config.optimization_phrase}</a></div>